<?xml version="1.0" encoding="utf-8"?>
<interface>
    <requires lib="gtk" version="4.0"/>
    <requires lib="adw" version="1.0"/>
    <object class="AdwActionRow" id="_root">
        <property name="title-lines">1</property>
        <child type="prefix">
            <object class="GtkLabel" id="_number">
            </object>
        </child>
        <child type="prefix">
            <object class="GtkImage" id="_status">
            </object>
        </child>
        <child type="suffix">
            <object class="GtkButton" id="_queue">
                <property name="visible">false</property>
                <style>
                    <class name="circular"/>
                </style>
            </object>
        </child>
        <child type="suffix">
            <object class="GtkLabel" id="_runtime">
            </object>
        </child>
    </object>
</interface>