<?xml version="1.0" encoding="utf-8"?>
<interface>
    <object class="AdwActionRow" id="_root">
        <property name="title-lines">1</property>
        <property name="subtitle-lines">1</property>
        <child type="prefix">
            <object class="GtkImage" id="_albumArt">
                <property name="overflow">hidden</property>
                <style>
                    <class name="icon-dropshadow"/>
                    <class name="album-art-small"/>
                </style>
                <property name="width-request">40</property>
                <property name="height-request">40</property>
                <property name="margin-end">8</property>
            </object>
        </child>
        <child type="suffix">
            <object class="GtkLabel" id="_duration">
            </object>
        </child>
    </object>
</interface>